      external_net: { list_join: ['-', [ 'external_net', { get_param: env_name } ]] }
      salt_master_control_ip: { get_param: salt_master_control_ip }
      management_subnet_cfg01_ip: { get_param: management_subnet_cfg01_ip }
      tenant_net_static_ip:
        list_join:
